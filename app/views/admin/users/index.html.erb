<h2>Users</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.id, user %></td>
            <td><%= user.email %></td>
          </tr>
        <% end %>
    </tbody>
</table>